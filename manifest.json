{
  "name": "Launch Survey Helper (beta)",
  "version": "0.1",
  "description": "The bare-bones of an app to help with Launch Survey creation",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Launch Survey Helper",
    "default_popup": "popup.html"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" 
  }
}